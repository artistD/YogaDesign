<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.will_d.yogadesign">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.YogaDesign">
        <activity
            android:name=".LoginActivity"
            android:exported="true"
            android:theme="@style/LoginActivity"/>

        <activity
            android:name=".IntroActivity"
            android:exported="true"
            android:theme="@style/IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".TrafficActivity"
            android:exported="true"
            android:theme="@style/TrafficActivity" />

        <activity
            android:name=".SquareActivity"
            android:exported="true"
            android:theme="@style/SquareActivity" />

        <activity
            android:name=".SetActivity"
            android:exported="true"
            android:theme="@style/SetActivity" />

        <activity
            android:name=".StatisticalActivity"
            android:exported="true"
            android:theme="@style/StatisticalActivity" />

        <activity
            android:name=".AlarmActivity"
            android:exported="true"
            android:theme="@style/AlarmActivity" />

        <activity
            android:name=".WorkShopActivity"
            android:exported="true"
            android:theme="@style/WorkShopActivity" />

        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.YogaDesign">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>